{
  "name": "SleepoLingo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "DEBUG=* node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "precommit": "npm run git-hook",
    "prepush": "npm run git-hook",
    "git-hook000": "npm run lint -s && npm run test -s",
    "git-hook": "npm run lint -s",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "postinstall": "rndebugger-open",
    "revert-debug": "rndebugger-open --revert"
  },
  "dependencies": {
    "@nozbe/watermelondb": "^0.10.1",
    "@nozbe/with-observables": "^1.0.2",
    "apisauce": "^1.0.2",
    "date-fns": "^1.30.1",
    "debug": "^4.1.1",
    "format-json": "^1.0.3",
    "ignite-andross": "^3.0.4",
    "ignite-i18n": "^1.1.1",
    "lodash": "^4.17.11",
    "md5-hex": "^2.0.0",
    "moment": "^2.22.2",
    "native-base": "^2.11.0",
    "p-limit": "^2.1.0",
    "prop-types": "^15.6.2",
    "querystringify": "1.0.0",
    "ramda": "^0.24.1",
    "react": "^16.6.3",
    "react-native": "^0.58.5",
    "react-native-action-button": "^2.8.5",
    "react-native-animatable": "^1.3.0",
    "react-native-background-timer": "^2.1.1",
    "react-native-collapsible": "^0.9.0",
    "react-native-config": "^0.11.7",
    "react-native-debugger-open": "^0.3.17",
    "react-native-dialogs": "0.0.19",
    "react-native-drawer": "^2.5.1",
    "react-native-elements": "^1.0.0",
    "react-native-flip-card": "^3.5.2",
    "react-native-fs": "^2.13.3",
    "react-native-gesture-handler": "^1.0.15",
    "react-native-i18n": "^2.0.15",
    "react-native-linear-gradient": "^2.5.3",
    "react-native-modalbox": "^1.7.1",
    "react-native-picker": "git+https://github.com/ssomnoremac/react-native-picker.git",
    "react-native-progress": "^3.5.0",
    "react-native-simple-toast": "0.0.8",
    "react-native-sound": "^0.10.12",
    "react-native-swiper-animated": "git+https://github.com/cwagner22/react-native-swiper-animated.git",
    "react-native-vector-icons": "^6.2.0",
    "react-navigation": "^3.3.2",
    "react-navigation-redux-helpers": "^2.0.9",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.2",
    "redux-saga-rn-alert": "^1.2.7",
    "reduxsauce": "^1.0.1",
    "rn-fetch-blob": "^0.10.15",
    "seamless-immutable": "^7.1.4",
    "shallowequal": "^1.1.0",
    "snazzy": "^7.1.1",
    "traverse": "^0.6.6",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "babel-eslint": "8.0.0",
    "babel-jest": "^24.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "^4.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "husky": "^0.14.3",
    "ignite-animatable": "^1.0.0",
    "ignite-i18n": "^1.0.0",
    "ignite-standard": "^1.0.0",
    "ignite-vector-icons": "^1.0.0",
    "jest": "^24.1.0",
    "lodash.clonedeep": "^4.5.0",
    "lokijs": "^1.5.6",
    "metro-react-native-babel-preset": "^0.52.0",
    "mock-fs": "^4.8.0",
    "mockery": "^2.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.8.3",
    "react-native-debugger-open": "^0.3.11",
    "react-test-renderer": "^16.8.3",
    "reactotron-react-native": "^3.0.0-beta.2",
    "reactotron-redux": "^3.0.0-beta.3",
    "reactotron-redux-saga": "^4.1.0",
    "redux-saga-test-plan": "^4.0.0-beta.2",
    "snazzy": "^7.0.0",
    "source-map-support": "^0.5.10",
    "standard": "10.0.3"
  },
  "jest": {
    "globals": {
      "__TEST__": true
    },
    "testMatch": [
      "**/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "Tests/Setup.js"
    ],
    "setupFiles": [
      "./Tests/Setup"
    ],
    "preset": "react-native"
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  },
  "rnpm": {
    "assets": [
      "App/Fonts",
      "App/lessons.xlsx"
    ]
  }
}
